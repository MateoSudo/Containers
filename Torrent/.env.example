# Private Internet Access VPN Credentials
# Copy this file to .env and fill in your actual credentials
PIA_USER=your_pia_username
PIA_PASS=your_pia_password

# Optional: Set specific PIA server location
# Examples:
# LOC=us_california
# LOC=uk_london
# LOC=us_east
# LOC=auto (for automatic selection)

# Drone CI/CD Configuration
DRONE_GITHUB_CLIENT_ID=your_github_oauth_app_client_id
DRONE_GITHUB_CLIENT_SECRET=your_github_oauth_app_client_secret
DRONE_RPC_SECRET=your_super_secret_shared_secret_here
DRONE_SERVER_HOST=localhost:8082
DRONE_SERVER_PROTO=http
DRONE_USER_CREATE=username:your_github_username,admin:true
